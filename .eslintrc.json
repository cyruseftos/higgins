{
  "root": true,
  "env": {
    "node": true
  },
  "extends": "airbnb-base/legacy",
  "rules": {
    "comma-dangle": [1, "always-multiline"],
    "no-console": 0,
    "space-before-function-paren": [2, { "anonymous": "always", "named": "never" }],
    "valid-jsdoc": [2, { "requireReturn": false }],
    "vars-on-top": 1
  }
}
